  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^16.0.0",
    "babel-preset-es2015": "^6.18.0",
